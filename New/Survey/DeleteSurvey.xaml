<Window x:Class="Survey.DeleteSurvey"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Survey"
        mc:Ignorable="d"
        Title="Delete Survey" Height="300" Width="300">
    <Grid>
        <Label Content="Id:" HorizontalAlignment="Left" Margin="19,18,0,0" VerticalAlignment="Top"/>
        <Label Content="Title:" HorizontalAlignment="Left" Margin="19,75,0,0" VerticalAlignment="Top"/>

        <TextBox HorizontalAlignment="Left" Height="23" Margin="66,75,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="162"/>
        <Button Name="btnCancelDeleteSurvey" Click="btnCancelDeleteSurvey_Click" Content="Cancel" HorizontalAlignment="Left" Margin="54,158,0,0" VerticalAlignment="Top" Width="75"/>
        <Button Name="btnDeleteSurvey" Click="btnDeleteSurvey_Click" Content="Delete" HorizontalAlignment="Left" Margin="170,158,0,0" VerticalAlignment="Top" Width="75"/>
        <TextBox Name="txtSurvey" HorizontalAlignment="Left" Height="23" Margin="66,21,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="90"/>
        <Button Name="btnSearchSurvey" Click="btnSearchSurvey_Click" Content="Search" HorizontalAlignment="Left" Margin="170,21,0,0" VerticalAlignment="Top" Width="75"/>

    </Grid>
</Window>
